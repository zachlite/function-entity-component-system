    "regl": "^1.3.9",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "teapot": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/gl-matrix": "^2.4.4",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.9",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-client": "^1.4.32",
    "nodemon": "^1.18.6",
